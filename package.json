{
  "name": "svelte-pwa-template",
  "version": "1.0.0",
  "author": "Todor Imreorov",
  "description": "a pwa starter template for svelte",
  "main": "main.js",
  "build": {
    "appId": "example.app"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "deploy": "yarn run build && gh-pages -d public",
    "electron": "NODE_ENV=development electron main.js",
    "build-linux": "electron-builder --linux --config electron-builder-config.json --publish never"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "gh-pages": "^3.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-workbox": "^5.0.1",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "workbox-sw": "^5.1.3"
  }
}
